{
  "mcpServers": {
    "prisma-mcp-server": {
      "command": "npx",
      "args": ["-y", "prisma", "mcp"]
    },
    "excel-mcp-server": {
      "command": "npx",
      "args": ["--yes", "@negokaz/excel-mcp-server"],
      "env": {
        "EXCEL_MCP_PAGING_CELLS_LIMIT": "4000"
      }
    }
  }
}
